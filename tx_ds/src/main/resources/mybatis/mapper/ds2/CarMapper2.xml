<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.lab.dao.mapper.ds2.CarMapper2">

    <resultMap id="BaseResultMap" type="org.lab.dao.model.Car">
        <id column="id" jdbcType="BIGINT" property="id"/>
        <result column="car_no" jdbcType="VARCHAR" property="carNo"/>
    </resultMap>

    <select id="pageQuery" resultMap="BaseResultMap">
        select *
        from car
        order by id
        limit #{limit} offset #{offset}
    </select>

    <insert id="insert">
        INSERT INTO car( car_no ) VALUES
        <foreach collection="cars" item="car" separator=",">
            (#{car.carNo})
        </foreach>
    </insert>

    <delete id="deleteByCarNos">
        delete from car where car_no in
        <foreach collection="carNos" item="carNo" separator="," open="(" close=")">
            #{carNo}
        </foreach>
    </delete>
</mapper>